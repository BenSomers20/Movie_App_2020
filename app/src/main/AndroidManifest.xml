<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="org.wit.movie">

    <application
        android:name="org.wit.movie.main.MainApp"
        android:allowBackup="true"
        android:icon="@mipmap/movielogo"
        android:label="Movie"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity android:name="org.wit.movie.activities.MovieListActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="org.wit.movie.activities.MovieActivity"></activity>
    </application>

</manifest>